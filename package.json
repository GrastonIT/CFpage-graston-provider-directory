{
  "name": "remix-vite-cf-vx",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:wrangler": "wrangler pages dev",
    "generate": "drizzle-kit generate",
    "db:migrate:local": "wrangler d1 migrations apply --local db",
    "db:migrate:prod": "wrangler d1 migrations apply --remote db",
    "db:fc": "npm run generate && npm run db:migrate:local && npm run db:migrate:prod",
    "cf-typegen": "wrangler types",
    "build": "react-router build",
    "start": "wrangler pages dev ./build/client",
    "preview": "npm run build && wrangler pages dev --local",
    "lint": "biome lint",
    "typecheck": "npm run cf-typegen && react-router typegen && tsc",
    "fmt": "biome format --write ./",
    "deploy": "npm run build && wrangler pages deploy"
  },
  "commandsDescription": {
    "dev": "Run development server",
    "dev:wrangler": "Run development server with wrangler",
    "generate": "Generate database schema",
    "db:migrate:local": "Run database migrations locally",
    "db:migrate:prod": "Run database migrations in production",
    "db:fc": "Run database migrations for both local and production",
    "cf-typegen": "Generate Cloudflare types",
    "build": "Build production assets",
    "start": "Run production server",
    "preview": "Run production server in preview mode",
    "lint": "Run linter",
    "typecheck": "Run type checker",
    "fmt": "Run formatter",
    "deploy": "Deploy production assets"
  },
  "dependencies": {
    "@react-router/cloudflare": "^7.0.0",
    "bcryptjs": "^3.0.2",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.41.0",
    "framer-motion": "^12.6.0",
    "isbot": "^5.1.25",
    "jose": "^6.0.10",
    "kysely": "^0.27.6",
    "kysely-d1": "^0.3.0",
    "leaflet": "^1.9.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-leaflet": "^5.0.0",
    "react-router": "^7.0.0",
    "xu-lib-error-checker": "^0.1.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@cloudflare/workers-types": "^4.20250321.0",
    "@react-router/dev": "^7.0.0",
    "@types/leaflet": "^1.9.20",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "drizzle-kit": "^0.30.5",
    "eslint": "^9.23.0",
    "eslint-import-resolver-typescript": "^4.2.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^3.114.11"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
